{
  "name": "p1-forms",
  "version": "1.0.0",
  "description": "CLI tool for exporting and importing PingOne Forms between environments",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "p1-forms": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pingone",
    "forms",
    "cli",
    "export",
    "import"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "commander": "^11.0.0",
    "inquirer": "^9.2.7",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}